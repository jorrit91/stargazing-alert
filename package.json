{
  "name": "node-with-linting",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "test": "jest --watch",
    "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "start": "nodemon dist/index.js",
    "heroku-postbuild": "node build --prod",
    "start:prod": "nodemon src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.10.5",
    "@types/node-fetch": "^2.5.4",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "jest": "^24.3.0",
    "nodemon": "^1.18.10"
  },
  "engines": {
    "node": "12.x"
  }
}
